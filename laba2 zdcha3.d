#include <stdio.h>
#include <math.h>

int main() {
    int x, y;
    double f;

    // Введення даних
    printf("Введіть значення цілого числа x: ");
    scanf("%d", &x);

    printf("Введіть значення цілого числа y: ");
    scanf("%d", &y);

    // Перевірка, щоб уникнути ділення на 0 та кореня з від’ємного числа
    if ((x + y) < 0) {
        printf("Помилка: корінь з від’ємного числа (x + y < 0).\n");
        return 1;
    }
    if (x == 0) {
        printf("Помилка: ділення на нуль у виразі (x^2).\n");
        return 1;
    }

    // Обчислення функції
    f = ((double)(x * x + y * y) / sqrt(x + y)) + ((double)(x - y) / (x * x));

    // Виведення результату
    printf("Результат обчислення функції f: %.4f\n", f);

    return 0;
}